    "illuminate/support": "*",
    "illuminate/database": "*",
    "illuminate/console": "*",
    "illuminate/http": "*",
    "sebastian/diff": "*",
    "ankitpokhrel/tus-php": "^2.4",
    "laravel-zero/foundation": "^12.40",
